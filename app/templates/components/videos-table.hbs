{{#each filterVideos as |videos index|}}
  <div class="row-4 clearfix">
    {{#each videos as |video _index|}}
      {{#link-to 'video-detail' video class='col-1-4'}}
        <img src="{{video.thmbnailLink}}" width="100%" height="17%" frameborder = "1">
      {{/link-to}}
    {{/each}}
  </div>
{{/each}}

<footer>
  <button class="previous-btn"  {{action 'previousPage'}}>Previous</button>
  <span> {{pageCurrentIndex}} </span>
  <button class="next-btn"  {{action 'nextPage'}}>Next</button>
</footer>
